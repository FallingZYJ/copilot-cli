  "version": "0.2.0",
  "configurations": [
    {
      "name": "Browse Lite: Attach",
      "type": "browse-lite",
      "request": "attach"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Browse Lite: Launch",
      "type": "browse-lite",
      "request": "launch",
      "url": "http://localhost:3000"
    }
  ]
}
